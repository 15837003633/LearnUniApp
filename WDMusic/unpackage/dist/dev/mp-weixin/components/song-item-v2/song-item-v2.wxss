.song-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
  padding-bottom: 12rpx;
}
.song-item .info {
  flex: 1;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.song-item .info .name {
  font-size: 15px;
  color: black;
}
.song-item .info .author {
  position: relative;
  padding-left: 40rpx;
  font-size: 12px;
  color: #aaa;
}
.song-item .info .author::before {
  content: "";
  position: absolute;
  left: 0rpx;
  top: 8rpx;
  width: 28rpx;
  height: 16rpx;
  background-size: cover;
  background-image: url("data:image/png;base64,");
}